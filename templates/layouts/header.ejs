<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Hotwire for Frontend Developers</title>
  <script src="/hotwire/javascript/turbo.es2017-esm.js" data-turbo-track="reload" type="module"></script>
  <script data-turbo-track="reload" type="module">
    import { Application } from "/hotwire/javascript/stimulus.js"
    import PopupController from "/hotwire/javascript/popup_controller.js"
    import FormlikeController from "/hotwire/javascript/formlike_controller.js"
    window.Stimulus = Application.start()

    Stimulus.register("popup", PopupController)
    Stimulus.register("formlike", FormlikeController)
  </script>
  <link rel="stylesheet" href="/hotwire/styles/output.css" data-turbo-track="reload">
</head>
<body>
<div class="bg-white flex justify-between p-1" data-controller="hello">
  <a href="/" class="text-lg p-1 rounded bg-indigo-600 text-white">
    Top</a>
  <div class="text-lg p-1 rounded bg-amber-600 text-white">Hotwire</div>
</div>

